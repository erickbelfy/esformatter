{
    "indent" : {
        "ChainedMemberExpression" : 0
    },
    "lineBreak": {
      "before": {
        "CallExpression": ">=1"
      },
      "after": {
        "CallExpression": ">=1"
      }
    },
    "whiteSpace" : {
        "before" : {
            "ArgumentList" : 1,
            "ArgumentComma" : 1
        },
        "after" : {
            "ArgumentList" : 1,
            "ArgumentComma" : 0
        }
    }
}
